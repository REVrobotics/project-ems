{
    "name": "project-ems",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "postinstall": "husky install"
    },
    "dependencies": {
        "husky": "^8.0.1",
        "lerna": "^5.1.8"
    },
    "workspaces": [
        "front-end/*",
        "back-end/*"
    ],
    "husky": {
        "hooks": {
          "pre-commit": "lerna run --concurrency 1 --stream precommit --since HEAD --exclude-dependents"
        }
    }
}
